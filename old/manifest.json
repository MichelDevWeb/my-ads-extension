{
  "manifest_version": 3,
  "name": "MDW Google Ads Account Manager",
  "version": "1.0",
  "description": "Manage Google Ads accounts and create MCC children",
  "permissions": ["identity", "identity.email", "storage"],
  "oauth2": {
    "client_id": "433583771675-s8q43doerjb2ats95iskgcpe9tqjtmb4.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/adwords"
    ]
  },
  "host_permissions": [
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*"
  ],
  "action": {
    "default_title": "MDW Google Ads Account Manager"
  },
  "background": {
    "service_worker": "background.js"
  },
  "key": ""
}
